{
  "name": "favasconcelos.github.io",
  "homepage": "https://avasconcelos.com",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "node-sass": "4.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-scripts": "3.1.1",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "2.0.1",
    "gh-pages": "2.1.1",
    "husky": "3.0.4",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b master -d build --git $(which git)",
    "eject": "react-scripts eject",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
